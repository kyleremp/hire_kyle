<h1>Create User</h1>

<table>
	<%=form_for(:admin_user, :url => {:action => 'create'}) do |f| %>
	<tr>
		<td>Name: </td>
		<td><%= f.text_field :name %></td>
	</tr>
		<tr>
		<td>username: </td>
		<td><%= f.text_field :username %></td>
	</tr>
		<tr>
		<td>email: </td>
		<td><%= f.email_field :email %></td>
	</tr>
		<tr>
		<td>password: </td>
		<td><%= f.password_field :password %></td>
	</tr>
	<tr>
		<td colspan="2"><%= f.submit %></td>
	</tr>
	<%end%>
</table>
